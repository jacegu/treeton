require 'treeton/grammar/whitespace'

module Treeton
  grammar Object
    include Whitespace

    rule open_curly_brace
      whitespace* '{' whitespace*
    end

    rule close_curly_brace
      whitespace* '}' whitespace*
    end

    rule colon
      whitespace* ':' whitespace*
    end

    rule comma
      whitespace* ',' whitespace*
    end
  end
end
